{
  "manifest_version": 2,
  "name": "Allerginator",
  "description": "Adds a right-click menu item to images to detect text, labels and faces.",
  "version": "0.0.9",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardWrite",
    "contextMenus",
    "notifications",
    "file://*",
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://primenow.amazon.com/*"
      ],
      "js": [
        "content1.js",
        "content2.js",
        "popup.js"
      ],
      "run_at":"document_end"
    } 
  ],
  "web_accessible_resources": [
    "exclamation-mark.png",
    "checked-mark.png"
  ],
  "background": {
    "scripts": ["background.js"]
  },
